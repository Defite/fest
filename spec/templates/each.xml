<?xml version="1.0"?>
<fest:template xmlns:fest="http://fest.mail.ru" context_name="json">
	<fest:script>var itrerate = {"foo": "bar"}</fest:script>
	<fest:each iterate="itrerate" index="i">
		<fest:value>i</fest:value>=<fest:value>itrerate[i]</fest:value>
	</fest:each>
	<fest:each iterate="itrerate" index="i" value="v">
		<fest:value>i</fest:value>=<fest:value>v</fest:value>
	</fest:each>
</fest:template>
